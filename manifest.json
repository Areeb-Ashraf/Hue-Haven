{
    "manifest_version": 3,
    "name": "Hue Haven",
    "description": "An extension that helps make webpages colorblind friendly",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "icons": {
      "16": "images/HueHavenIcon.png"
    },
    "action": {
      "default_popup": "index.html"
    }
  }